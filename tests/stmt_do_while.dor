/*
 * stmt_do_while.dor
 * do while / exit / loop examples
 */

procedure main()

   local n

   n := 0
   do while n < 3
      n := n + 1
      if n == 2
         loop
      endif
      printf( n )
   enddo

   n := 0
   do while n < 10
      n := n + 1
      if n == 4
         exit
      endif
      printf( n )
   enddo

return

